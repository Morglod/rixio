{
  "name": "@rixio/rxjs-react-native",
  "version": "0.1.0",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "keywords": [
    "frp",
    "lens",
    "react",
    "reactive",
    "rx",
    "rxjs",
    "atom"
  ],
  "scripts": {
    "build": "npm run lint && npm run build-lib",
    "build-lib": "rimraf ./build && tsc && npm run copy-assets",
    "copy-json-to-lib": "cpx \"./src/**/*.json\" ./build",
    "copy-assets": "npm run copy-json-to-lib",
    "test": "jest",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:types": "tsc --noEmit",
    "lint": "npm run lint:js && npm run lint:types",
    "lint:fix": "eslint --ext .js,.ts,.ts,.tsx ./ --fix --quiet",
    "ci": "sh ./scripts/ci.sh",
    "release": "sh ./scripts/release.sh"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/roborox/rixio.git"
  },
  "devDependencies": {
    "@rixio/lens": "^0.1.0",
    "@rixio/rxjs-react": "^0.1.0",
    "@roborox/eslint-config-default": "2.1.4",
    "@testing-library/jest-native": "3.1.0",
    "@types/jest": "26.0.0",
    "@types/react": "16.9.37",
    "@types/react-native": "0.62.13",
    "@types/react-test-renderer": "16.9.2",
    "eslint": "7.2.0",
    "jest": "26.0.1",
    "react": "16.13.1",
    "react-native": "0.62.2",
    "react-native-testing-library": "2.1.0",
    "react-test-renderer": "16.13.1",
    "rxjs": "6.5.5",
    "ts-jest": "26.1.1",
    "typescript": "3.9.5"
  },
  "peerDependencies": {
    "@rixio/lens": ">= 0.0.1",
    "@rixio/rxjs-react": ">= 0.0.1",
    "react": ">= 16.9.0",
    "react-native": ">= 0.62.0",
    "rxjs": ">= 6.5.0 < 7.0.0-0"
  }
}
