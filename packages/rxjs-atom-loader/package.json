{
  "name": "@grecha/rxjs-atom-loader",
  "version": "0.0.1",
  "keywords": [
    "frp",
    "lens",
    "reactive",
    "rx",
    "rxjs",
    "atom"
  ],
  "scripts": {
    "build": "npm run lint && npm run build-lib",
    "build-lib": "rimraf ./build && tsc && npm run copy-assets",
    "copy-json-to-lib": "cpx \"./src/**/*.json\" ./build",
    "copy-assets": "npm run copy-json-to-lib",
    "test": "jest",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:types": "tsc --noEmit",
    "lint": "npm run lint:js && npm run lint:types",
    "lint:fix": "eslint --ext .js,.ts,.ts,.tsx ./ --fix --quiet",
    "ci": "sh ./scripts/ci.sh",
    "release": "sh ./scripts/release.sh"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/roborox/grecha.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@grecha/rxjs-atom": "0.0.1",
    "@grecha/rxjs-react": "0.0.1",
    "@roborox/eslint-config-default": "2.1.4",
    "@testing-library/jest-dom": "5.10.1",
    "@testing-library/react": "10.2.1",
    "@types/jest": "26.0.0",
    "eslint": "7.2.0",
    "jest": "26.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rxjs": "6.5.5",
    "ts-jest": "26.1.0",
    "typescript": "3.9.5"
  },
  "peerDependencies": {
    "rxjs": ">= 6.5.0 < 7.0.0-0",
    "@grecha/lens": ">= 0.0.1",
    "@grecha/rxjs-atom": ">= 0.0.1",
    "@grecha/rxjs-react": ">= 0.0.1"
  }
}
